{
    "description": "Auto Fullscreen for Firefox",
    "manifest_version": 2,
    "name": "AutoFullscreen",
    "version": "2.0",
    "homepage_url": "https://github.com/Insiderser/AutoFullscreen",
    "background": {
        "scripts": [
            "startup.js"
        ]
    },
    "icons": {
        "48": "icons/fullscreen_black_48x48.png",
        "96": "icons/fullscreen_black_96x96.png"

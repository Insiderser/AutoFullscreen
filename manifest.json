{
    "description": "Auto Fullscreen for Firefox",
    "manifest_version": 2,
    "name": "AutoFullscreen",
    "version": "2.0",
    "homepage_url": "https://github.com/Insiderser/AutoFullscreen",
    "background": {
        "scripts": [
            "startup.js"
        ]
    },
    "icons": {
